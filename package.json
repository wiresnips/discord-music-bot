{
  "name": "discord-musicbot",
  "version": "0.1.5",
  "description": "A music bot for Discord servers, self-hosted and easy to use.",
  "keywords": [
    "discord",
    "bot",
    "music",
    "musicbot",
    "radio"
  ],
  "homepage": "https://github.com/South-Paw/discord-music-bot",
  "bugs": {
    "url": "https://github.com/South-Paw/discord-music-bot/issues",
