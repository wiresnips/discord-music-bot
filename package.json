    "name": "Alex Gabites",
    "email": "hello@southpaw.co.nz",
    "url": "http://southpaw.co.nz/"
  },
  "files": [
    "src"
  ],
  "main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/South-Paw/discord-music-bot"
  },
  "scripts": {
    "lint": "eslint ./src",
    "test": "jest",
    "test/coverage": "jest --coverage",
    "update-ytdl": "npm install --save ytdl-core@latest",
    "update-opus": "npm install --save @discordjs/opus@latest",
    "run": "npm run update-ytdl; npm run update-opus; node src/run.js"
  },
  "dependencies": {
    "@discordjs/opus": "^0.4.0",
    "discord.js": "^11.3.0",
    "ffmpeg-binaries": "^3.2.2-3",
    "string-format": "^0.5.0",
    "url-parse": "^1.2.0",
    "ytdl-core": "^4.5.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.18.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.9.0",
    "jest": "^22.4.2"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
